@model List<UserViewModel>;

	<table class="table">
		<thead>
			<tr>
				<th>FirstName</th>
				<th>LastName</th>
				<th>Email</th>
				<th>Sex</th>
			</tr>
		</thead>
		<tbody>
			@foreach(var user in @Model){
				<tr>
					<td>@user.FirstName</td>
					<td>@user.LastName</td>
					<td>@user.Email</td>
					<td>@user.Sex</td>
				</tr>
			}
		</tbody>
	</table>